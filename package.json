{
  "name": "caisson",
  "version": "0.1.3",
  "author": "Christopher Cliff <ctcliff@gmail.com>",
  "description": "A pluggable cloud deployment utility",
  "homepage": "http://caisson.co",
  "repository": {
    "type": "git",
    "url": "http://github.com/christophercliff/caisson"
  },
  "bugs": {
    "url": "https://github.com/christophercliff/caisson/issues"
  },
  "keywords": [
    "aws",
    "s3",
    "route 53",
    "cloudfront",
    "digitalocean",
    "ssl",
    "https",
    "static site generator",
    "jekyll",
    "wintersmith",
    "metalsmith",
    "utility"
  ],
  "main": "./lib/",
  "scripts": {
    "test": "./node_modules/.bin/jshint ./lib/ ./test/ && ./node_modules/.bin/mocha --reporter spec --require should"
  },
  "dependencies": {
    "bluebird": "^1.1.1",
    "own": "^0.0.7",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "async": "^0.2.10",
    "jshint": "^2.4.4",
    "mocha": "^1.18.2",
    "should": "^3.2.0"
  },
  "jshintConfig": {
      "eqeqeq": true,
      "immed": true,
      "latedef": "nofunc",
      "newcap": true,
      "quotmark": "single",
      "trailing": true,
      "unused": true,
      "asi": true,
      "boss": true,
      "expr": true,
      "laxbreak": true,
      "laxcomma": true,
      "scripturl": true,
      "sub": true,
      "loopfunc": true
  }
}
